					<artifactId>MetaModel-openoffice</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.metamodel</groupId>
					<artifactId>MetaModel-pojo</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.metamodel</groupId>
					<artifactId>MetaModel-salesforce</artifactId>
				</exclusion>
